@using Bootstrap.Models;

@model IEnumerable<ElectricEnergyMeterModel>
<div class="top-buffer"></div>
<div class="panel panel-primary">
    <div class="panel-heading panel-head">Energy Meters</div>
    <div class="panel-body">
        <div class="top-buffer"></div>
        <table class="table table-dark">
            <thead>
                <tr>
                    <th>electricEnergyMeterNumber</th>
                    <th>electricEnergyMeterType</th>
                    <th>electricEnergyMeterDate</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@Html.DisplayFor(modelItem => item.electricEnergyMeterNumber)</td>
                        <td>@Html.DisplayFor(modelItem => item.electricEnergyMeterType)</td>
                        <td>@Html.DisplayFor(modelItem => item.electricEnergyMeterDate)</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>