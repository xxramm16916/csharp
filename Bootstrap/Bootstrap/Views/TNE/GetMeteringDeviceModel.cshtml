@using Bootstrap.Models;

@model IEnumerable<MeteringDeviceModel>
<div class="top-buffer"></div>
<div class="panel panel-primary">
    <div class="panel-heading panel-head">MeteringDevices</div>
    <div class="panel-body">
        <div class="top-buffer"></div>
        <table class="table table-dark">
            <thead>
                <tr>
                    <th>meteringDeviceId</th>
                    <th>meteringDeviceDateRegistration</th>
                    <th>meteringDeviceDateRemoval</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@Html.DisplayFor(modelItem => item.meteringDeviceId)</td>
                        <td>@Html.DisplayFor(modelItem => item.meteringDeviceDateRegistration)</td>
                        <td>@Html.DisplayFor(modelItem => item.meteringDeviceDateRemoval)</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>