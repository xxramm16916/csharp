@using Bootstrap.Models;

@model IEnumerable<CurrentTransformerModel>
<div class="top-buffer"></div>
<div class="panel panel-primary">
    <div class="panel-heading panel-head">Current Transformer</div>
    <div class="panel-body">
        <div class="top-buffer"></div>
        <table class="table table-dark">
            <thead>
                <tr>
                    <th>currentTransformerNumber</th>
                    <th>currentTransformerType</th>
                    <th>currentTransformerDate</th>
                    <th>currentTransformerKoeff</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@Html.DisplayFor(modelItem => item.currentTransformerNumber)</td>
                        <td>@Html.DisplayFor(modelItem => item.currentTransformerType)</td>
                        <td>@Html.DisplayFor(modelItem => item.currentTransformerDate)</td>
                        <td>@Html.DisplayFor(modelItem => item.currentTransformerKoeff)</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

